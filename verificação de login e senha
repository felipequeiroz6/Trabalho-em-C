#include <stdio.h>
#include <string.h>

int main() {
    char loginDigitado[20];
    char senhaDigitada[20];

    // Login e senha corretos (pré-definidos)
    char loginCorreto[] = "jujudopix";
    char senhaCorreta[] = "1234";

    // Pede o login
    printf("Digite o login: ");
    scanf("%s", loginDigitado);

    // Pede a senha
    printf("Digite a senha: ");
    scanf("%s", senhaDigitada);

    // Verifica se o login e a senha estão corretos
    if (strcmp(loginDigitado, loginCorreto) == 0 && strcmp(senhaDigitada, senhaCorreta) == 0) {
        printf("Login bem-sucedido!\n");
    } else {
        printf("Login ou senha incorretos.\n");
    }

    return 0;
}
